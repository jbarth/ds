package com.ubs.gtp.data.resource.client;

import java.math.BigDecimal;

/**
 * Incoming add client request.
 *
 * @author Mihkel Aamer
 * @author Joshua.Barth
 * @since 0.3
 */
public final class AddClientDto {

    private String name;
    private String surname;
    private BigDecimal credit;
    private String username;
    private Integer faAdvisor;

    public AddClientDto() {
    }

    public AddClientDto(final String name, final String surname, final BigDecimal credit, final String username, final Integer faAdvisor) {
        this.name = name;
        this.surname = surname;
        this.credit = credit;
        this.username = username;
        this.faAdvisor = faAdvisor;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getSurname() {
        return surname;
    }

    public void setSurname(String surname) {
        this.surname = surname;
    }

    public BigDecimal getCredit() {
        return credit;
    }

    public void setCredit(BigDecimal credit) {
        this.credit = credit;
    }

    public String getUsername() {
        return username;
    }

    public void setUsername(String username) {
        this.username = username;
    }

	public int getFaAdvisor() {
		return faAdvisor;
	}

	public void setFaAdvisor(int faAdvisor) {
		this.faAdvisor = faAdvisor;
	}
}
